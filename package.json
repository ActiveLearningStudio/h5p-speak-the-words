  "license": "MIT",
  "devDependencies": {
    "annyang": "^2.4.0",
    "babel-core": "^6.9.1",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "h5p-view": "^1.0.3",
    "imports-loader": "^0.6.5",
    "jest-cli": "^17.0.0",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
