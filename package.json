{
  "name": "voice-recognition-question",
  "version": "1.0.0",
  "description": "A language task where you have to answer a question using speech",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "testDev": "jest --watch",
    "build": "cross-env NODE_ENV=production webpack -p",
    "buildDev": "webpack --watch"
  },
  "keywords": [
    "language",
    "translation",
    "voice",
    "recognition",
    "mic",
    "microphone",
    "speech"
  ],
  "author": "Thomas Marstrander",
  "license": "MIT",
  "devDependencies": {
    "annyang": "^2.4.0",
    "babel-core": "^6.9.1",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-destructuring": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.25.0",
    "jest-cli": "^17.0.0",
    "react-addons-test-utils": "^15.1.0",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "he": "^1.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "^2.2.1"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "<rootDir>/tests/stylesModule.js"
    ],
    "moduleNameMapper": {
      "^.*[.](css|CSS)$": "<rootDir>/tests/stylesModule"
    }
  }
}
